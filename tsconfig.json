{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "inlineSources": true,
        "outDir": "dist",
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "index.d.ts",
        "dist"
    ]
}
