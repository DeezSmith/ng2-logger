{
  "name": "ng2-logger",
  "version": "1.0.5",
  "description": "logger for angular 2",
  "scripts": {
    "build": "npm-run ng-packagr -p package.json",
    "release": "cd dist && release-it",
    "pre:publish": "cp screen.png /dist && cp README.md dist/ && cp package.json dist/"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "keywords": [
    "angular2",
    "logger",
    "log"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "devDependencies": {
    "ng-packagr": "^1.6.0",
    "typescript": "^2.2.2"
  },
  "readmeFilename": "README.md"
}
